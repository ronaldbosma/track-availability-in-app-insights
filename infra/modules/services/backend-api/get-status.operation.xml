<policies>
    <inbound>
        <base />

        <!-- 
            Add rate limit so calls from the availability tests will fail every ~ 5 minutes if they execute 5 times or more in that period.
            Rate limit applies per subscription key. It's also applies to calls without a subscription key.
        -->
        <rate-limit calls="4" renewal-period="300" retry-after-header-name="Retry-After" remaining-calls-header-name="Remaining-Calls" total-calls-header-name="Total-Calls" />

        <return-response>
            <set-status code="200" reason="OK" />
        </return-response>
    </inbound>

    <backend>
        <base />
    </backend>
    
    <outbound>
        <base />
    </outbound>
    
    <on-error>
        <base />
    </on-error>
</policies>